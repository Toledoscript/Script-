local Mercury = loadstring(game:HttpGet("https://raw.githubusercontent.com/deeeity/mercury-lib/master/src.lua"))()

local GUI = Mercury:Create{
    Name = "Rice Hub | Dunking Simulator",
    Size = UDim2.fromOffset(600, 400),
    Theme = Mercury.Themes.Dark,
    Link = "https://github.com/deeeity/mercury-lib"
}

GUI:Prompt{
	Followup = false,
	Title = "Thank you!",
	Text = "Thank you for using script Hub!",
	Buttons = {
		Close = function()
			return true
		end
	}
}

GUI:Notification{
	Title = "Alert",
	Text = "Please note that Script Hub is in Alpha stages, so bugs may appear!",
	Duration = 3,
	Callback = function() end
}

local Tab = GUI:Tab{
    Name = "Autofarms",
    Icon = "rbxassetid://7733970318"
}

Tab:Toggle{
    Name = "Auto Rebirth",
    StartingState = false,
    Description = "Very laggy but very op!",
    Callback = function(state) 
        while true do
            local ohInstance1 = game:GetService("Players").LocalPlayer.ownedShoes["2"]
            local ohNumber2 = 0
            local ohNumber3 = math.huge
            game:GetService("ReplicatedStorage").Remote.Shoes.SellShoes:FireServer(ohInstance1, ohNumber2, ohNumber3)
            game:GetService("ReplicatedStorage").Rebirth:FireServer()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/Aidanman14/RiceHub/extras/fix', true))()
            wait(0.1)
            end
    end
}

Tab:Toggle{
    Name = "Auto Season Pass",
    StartingState = false,
    Description = "Very slow",
    Callback = function(state) 
        while true do
local ohString1 = "WildSeasonalCash"

game:GetService("ReplicatedStorage").Remote.Orb.OrbScore:FireServer(ohString1)
            wait(0.1)
            end
    end
}

